{
    "name": "myapp",
    "project_slug": "{{cookiecutter.name|lower|replace('.', '-')}}",
    "tld_domain": "example.com",
    "app_suffix": "back",
    "dev_front_domain": "dev-terralego-{{cookiecutter.name}}.{{cookiecutter.tld_domain}}",
    "dev_back_domain": "dev-terralego-{{cookiecutter.name}}.{{cookiecutter.tld_domain}}",
    "qa_front_domain": "qa-terralego-{{cookiecutter.name}}.{{cookiecutter.tld_domain}}",
    "qa_back_domain": "qa-terralego-{{cookiecutter.name}}.{{cookiecutter.tld_domain}}",
    "staging_front_domain": "staging-terralego-{{cookiecutter.name}}.{{cookiecutter.tld_domain}}",
    "staging_back_domain": "staging-terralego-{{cookiecutter.name}}.{{cookiecutter.tld_domain}}",
    "prod_front_domain": "terralego-{{cookiecutter.name}}.{{cookiecutter.tld_domain}}",
    "prod_back_domain": "terralego-{{cookiecutter.name}}.{{cookiecutter.tld_domain}}",
    "staging_host": "{{cookiecutter.staging_front_domain.replace('-{0}.'.format(cookiecutter.name), '-{0}-docker.'.format(cookiecutter.name))}}",
    "qa_host": "{{cookiecutter.qa_front_domain.replace('-{0}.'.format(cookiecutter.name), '-{0}-docker.'.format(cookiecutter.name))}}",
    "dev_host": "{{cookiecutter.dev_front_domain.replace('-{0}.'.format(cookiecutter.name), '-{0}-docker.'.format(cookiecutter.name))}}",
    "prod_host": "{{cookiecutter.prod_front_domain.replace('-{0}.'.format(cookiecutter.name), '-{0}-docker.'.format(cookiecutter.name))}}",
    "staging_port": "22",
    "qa_port": "22",
    "dev_port": "22",
    "prod_port": "22",
    "mail_domain": "{{cookiecutter.tld_domain }}",
    "infra_domain": "{{cookiecutter.tld_domain }}",
    "git_server": "gitlab.{{cookiecutter.infra_domain}}",
    "git_project_server": "{{cookiecutter.git_server}}",
    "git_url": "https://{{cookiecutter.git_server}}",
    "git_ns": "Terralego/Customers/{{cookiecutter.name}}",
    "git_project_url": "https://{{cookiecutter.git_project_server}}/{{cookiecutter.git_ns}}/{{cookiecutter.name}}-back",
    "deploy_project_url": "https://github.com/makinacorpus/terra-back-deploy.git",
    "deploy_project_dir": "local/{{cookiecutter.deploy_project_url.split('/')[-1].replace('.git', '')}}",
    "egg_project_url": "../../../../Terralego/Core/terra-back.git",
    "egg_project_dir": "lib/{{cookiecutter.egg_project_url.split('/')[-1].replace('.git', '')}}",
    "docker_registry": "registry.{{cookiecutter.infra_domain}}",
    "simple_docker_image": "{{cookiecutter.git_ns.split('/')[0]}}/{{cookiecutter.name}}{{cookiecutter.app_suffix}}",
    "docker_image": "{%if cookiecutter.docker_registry%}{{cookiecutter.docker_registry}}/{%endif%}{{cookiecutter.simple_docker_image}}"
}
