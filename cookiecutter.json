{
    "name": "myapp",
    "lname": "{{cookiecutter.name|lower}}",
    "out_dir": ".",
    "tld_domain": "example.com",
    "app_type": "django",
    "app_suffix": "-back",
    "use_submodule_for_deploy_code": "y",
    "django_project_name": "project",
    "django_settings": "{{cookiecutter.django_project_name}}.settings",

    "dev_back_domain": "dev-terralego-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "dev_front_domain": "{{cookiecutter.dev_back_domain}}",
    "dev_tiles_domain": "a-dev-tiles-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",

    "qa_back_domain": "qa-terralego-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "qa_front_domain": "{{cookiecutter.qa_back_domain}}",
    "qa_tiles_domain": "a-qa-tiles-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",

    "staging_back_domain": "staging-terralego-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "staging_front_domain": "{{cookiecutter.staging_back_domain}}",
    "staging_tiles_domain": "a-staging-tiles-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",

    "prod_back_domain": "terralego-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",
    "prod_front_domain": "{{cookiecutter.prod_back_domain}}",
    "prod_tiles_domain": "a-tiles-{{cookiecutter.lname}}.{{cookiecutter.tld_domain}}",

    "staging_host": "{{cookiecutter.staging_front_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "qa_host": "{{cookiecutter.qa_front_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "dev_host": "{{cookiecutter.dev_front_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "prod_host": "{{cookiecutter.prod_front_domain.replace('-{0}.'.format(cookiecutter.lname), '-docker-{0}.'.format(cookiecutter.lname))}}",
    "staging_port": "22",
    "remove_cron": "",
    "qa_port": "22",
    "dev_port": "22",
    "prod_port": "22",
    "mail_domain": "{{cookiecutter.tld_domain }}",
    "infra_domain": "{{cookiecutter.tld_domain }}",
    "git_server": "gitlab.{{cookiecutter.infra_domain}}",
    "git_project_server": "{{cookiecutter.git_server}}",
    "git_url": "https://{{cookiecutter.git_server}}",
    "git_ns": "Terralego/Customers/{{cookiecutter.name}}",
    "git_project": "{{cookiecutter.lname}}{{cookiecutter.app_suffix}}",
    "git_project_url": "https://{{cookiecutter.git_project_server}}/{{cookiecutter.git_ns}}/{{cookiecutter.git_project}}",
    "test_tests": "y",
    "test_linting": "y",
    "fname_slug": "{{'{0}-{1}{2}'.format(cookiecutter.git_ns, cookiecutter.lname, cookiecutter.app_suffix)|lower|replace('/', '-')|replace('_', '-')}}",
    "runner_tag": "{{cookiecutter.fname_slug}}-ci",
    "deploy_project_url": "https://github.com/makinacorpus/terra{{cookiecutter.app_suffix}}-deploy.git",
    "deploy_project_dir": "local/{{cookiecutter.deploy_project_url.split('/')[-1].replace('.git', '')}}",
    "egg_project_url": "../../../../Terralego/Core/terra{{cookiecutter.app_suffix}}.git",
    "with_sentry": "y",
    "egg_project_dir": "lib/{{cookiecutter.egg_project_url.split('/')[-1].replace('.git', '')}}",
    "docker_registry": "registry.{{cookiecutter.infra_domain}}",
    "simple_docker_image": "{{cookiecutter.git_ns.split('/')[0].lower()}}/{{cookiecutter.lname}}{{cookiecutter.app_suffix.replace('-', '')}}",
    "docker_image": "{%if cookiecutter.docker_registry%}{{cookiecutter.docker_registry}}/{%endif%}{{cookiecutter.simple_docker_image}}",
    "postgis_image": "corpusops/pgrouting:10.1-2.5.4",
    "postgresql_image": "corpusops/postgres:10",
    "postgres_image": "{{cookiecutter.postgresql_image}}",
    "mysql_image": "corpusops/mysql",
    "redis_image": "corpusops/redis:4.0-alpine",
    "nginx_image": "corpusops/nginx:1.14-alpine",
    "db_mode": "postgis",
    "py_ver": "3.6"
}
